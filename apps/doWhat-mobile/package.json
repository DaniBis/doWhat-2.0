{
  "name": "doWhat-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client -c",
    "start:ios": "bash ./scripts/start-ios.sh",
    "start:android": "expo start --dev-client --android -c",
    "start:web": "expo start --web -c",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild:ios": "expo prebuild --clean --platform ios",
    "prebuild:android": "expo prebuild --clean --platform android",
    "pods": "cd ios && LANG=en_US.UTF-8 pod install && cd ..",
    "run:ios:sim": "pnpm run ios -- --device 'iPhone 16 Pro'",
    "run:android:emu": "pnpm run android -- --variant debug",
    "run:android:emu:arm64": "pnpm run android -- --variant debug -PreactNativeArchitectures=arm64-v8a",
    "run:android:emu:x86_64": "pnpm run android -- --variant debug -PreactNativeArchitectures=x86_64"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.7",
    "@dowhat/shared": "workspace:*",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "~1.21.0",
    "@rneui/base": "4.0.0-rc.8",
    "@rnmapbox/maps": "^10.2.1",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/react-query": "^5.59.0",
    "expo": "~50.0.17",
    "expo-auth-session": "~5.4.0",
    "expo-blur": "~12.9.2",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-dev-client": "~3.3.9",
    "expo-device": "~5.9.3",
    "expo-image-manipulator": "~11.8.0",
    "expo-image-picker": "~14.7.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-notifications": "~0.27.6",
    "expo-router": "3.4.10",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.7.2",
    "expo-web-browser": "~12.8.2",
    "invariant": "^2.2.4",
    "nativewind": "^4.1.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "~4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.12",
    "react-native-webview": "13.6.4",
    "tailwindcss-react-native": "^1.7.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@jest/globals": "^29.3.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "18.2.79",
    "@types/react-dom": "~18.0.11",
    "expo-build-properties": "^1.0.9",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "pretty-format": "^29.7.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.1"
  }
}
