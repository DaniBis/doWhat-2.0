{
  "name": "dowhat-monorepo",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "install:all": "pnpm install",
    "build": "pnpm -r --filter ./packages/shared build && pnpm -r --filter \"./apps/*\" build",
    "dev:web": "pnpm --filter doWhat-web dev",
    "dev:mobile": "pnpm --filter doWhat-mobile start",
    "clean": "pnpm -r --filter \"./**\" run clean || true",
    "typecheck": "pnpm -r --filter \"./**\" run typecheck || true",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14",
    "eslint-plugin-import": "2",
    "eslint-plugin-react": "7",
    "eslint-plugin-react-hooks": "4",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "expo-constants": "^16.0.2",
    "expo-device": "^6.0.2",
    "expo-maps": "^0.11.0",
    "expo-notifications": "^0.31.4"
  }
}
