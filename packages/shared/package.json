{
  "name": "@dowhat/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.74"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "rimraf": "6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "5.5.4"
  }
}
